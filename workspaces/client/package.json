{
  "name": "@wsh-2025/client",
  "private": true,
  "scripts": {
    "analyze": "vite build --mode analyze",
    "build": "wireit",
    "dev": "lsof -i :3000 -t | xargs -r kill -9 && react-router dev",
    "format": "wireit",
    "format:eslint": "wireit",
    "format:prettier": "wireit",
    "preview": "vite preview"
  },
  "dependencies": {
    "@better-fetch/fetch": "1.1.15",
    "@dhmk/zustand-lens": "5.0.0",
    "@epic-web/restore-scroll": "1.1.1",
    "@ffmpeg/ffmpeg": "0.12.15",
    "@headlessui/react": "2.2.0",
    "@radix-ui/react-slider": "1.2.3",
    "@react-router/node": "7.4.0",
    "@standard-schema/spec": "1.0.0",
    "@unocss/reset": "66.1.0-beta.6",
    "@wsh-2025/schema": "workspace:*",
    "@yornaath/batshit": "0.10.1",
    "classnames": "2.5.1",
    "deepmerge": "4.3.1",
    "final-form": "4.20.10",
    "isbot": "^5",
    "m3u8-parser": "7.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-ellipsis-component": "1.1.11",
    "react-final-form": "6.5.9",
    "react-flip-toolkit": "7.2.4",
    "react-router": "7.4.0",
    "react-router-dom": "7.4.0",
    "react-use": "17.6.0",
    "tiny-invariant": "1.3.3",
    "type-fest": "4.29.1",
    "use-callback-ref": "1.3.3",
    "use-sync-external-store": "1.4.0",
    "zod": "3.24.1",
    "zustand": "5.0.3",
    "zustand-di": "0.0.16"
  },
  "devDependencies": {
    "@ffmpeg/core": "0.12.10",
    "@ffmpeg/util": "0.12.2",
    "@iconify/json": "2.2.317",
    "@iconify/types": "2.0.0",
    "@react-router/dev": "7.4.0",
    "@types/m3u8-parser": "7.2.0",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.2",
    "@types/wicg-task-scheduling": "2024.1.0",
    "@vitejs/plugin-react": "4.3.4",
    "@wsh-2025/configs": "workspace:*",
    "arraybuffer-loader": "1.0.8",
    "babel-loader": "9.2.1",
    "hls.js": "1.5.17",
    "rollup-plugin-visualizer": "5.14.0",
    "typescript": "5.7.2",
    "unocss": "66.1.0-beta.6",
    "vite": "6.2.2",
    "vite-tsconfig-paths": "5.1.4",
    "webpack": "5.96.1",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.1.0",
    "wireit": "0.14.9"
  },
  "wireit": {
    "build": {
      "command": "vite build",
      "files": [
        "../../pnpm-lock.yaml",
        "../schema/src/**/*",
        "./src/**/*",
        "./vite.config.ts",
        "./uno.config.ts",
        "./index.html"
      ],
      "output": [
        "./dist/**/*"
      ]
    },
    "format": {
      "command": "pnpm run format:eslint && pnpm run format:prettier"
    },
    "format:eslint": {
      "command": "eslint --fix ."
    },
    "format:prettier": {
      "command": "prettier --write ."
    }
  }
}
